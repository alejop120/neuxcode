<p id="notice"><%= notice %></p>

<h1>Course Has Subjects</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Subject</th>
      <th>Teacher</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_has_subjects.each do |course_has_subject| %>
      <tr>
        <td><%= course_has_subject.course %></td>
        <td><%= course_has_subject.subject %></td>
        <td><%= course_has_subject.teacher %></td>
        <td><%= link_to 'Show', course_has_subject %></td>
        <td><%= link_to 'Edit', edit_course_has_subject_path(course_has_subject) %></td>
        <td><%= link_to 'Destroy', course_has_subject, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

